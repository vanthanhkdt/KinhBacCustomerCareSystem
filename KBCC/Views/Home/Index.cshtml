        function addAddAction() {
            let lastRow = $('.table-upload-orders tr:last');
            //console.log(lastRow);
            let aTag = $('<a>', { class: 'add-new' });
            aTag.append($('<i>', { class: 'fa fa-plus' }))
            aTag.on('click', function () {
                //tableOrder++;
                var tr = $('<tr>');
                var name = $('<input>', { class: 'form-control input-sm', type: 'text', placeholder: "Họ tên" });
                var phone = $('<input>', { class: 'form-control input-sm', type: 'text', placeholder: "SĐT" });
                var address = $('<input>', { class: 'form-control input-sm', type: 'text', placeholder: "Địa chỉ" });
                var internalNote = $('<input>', { class: 'form-control input-sm', type: 'text', placeholder: "Ghi chú nội bộ" });

                tr.append($('<td>', { class: 'order', text: getMaxOrder() }));
                tr.append($('<td>', { class: 'input-area' }).append(name));
                tr.append($('<td>', { class: 'input-area' }).append(phone));
                tr.append($('<td>', { class: 'input-area' }).append(address));
                tr.append($('<td>', { class: 'input-area' }).append(internalNote));

                let aCopy = $('<a>', { class: 'copy', title: 'Sao chép' });
                aCopy.append($('<i>', { class: 'fa fa-copy' }))
                aCopy.on('click', function () {
                    let $ctr = $(this).closest('tr');
                    let $trColone = $ctr.clone();
                    $trColone.insertAfter($ctr);
                    console.log($ctr);
                    $('.add-new').remove();
                    addAddAction();
                });
                let aDelete = $('<a>', { class: 'delete', title: 'Xóa' });
                aDelete.append($('<i>', { class: 'fa fa-trash' }))
                aDelete.on('click', function () {
                    tr.remove();

                    $('.add-new').remove();
                    addAddAction();
                });

                tr.append($('<td>', { class: 'import-excel-actions' }).append(aCopy).append(aDelete));

                tableBody.append(tr);
                $('.add-new').remove();
                addAddAction();
            });
            //console.log(aTag);
            lastRow.find('.order').append(aTag);
        }
        function getMaxOrder() {
            let lastRow = $('.table-upload-orders tr:last');
            let maxId = parseInt(lastRow.find('.order').text()) + 1;
            console.log(maxId);
            if (!isNaN(maxId)) {
                return maxId;
            }
            return 1;
        }
        function openTab(evt, tabName) {
            //alert(tabName);
            //console.log(evt.currentTarget);
            var i, x, tablinks;
            x = document.getElementsByClassName("m-tab-content");
            for (i = 0; i < x.length; i++) {
                x[i].style.display = "none";
            }
            tablinks = document.getElementsByClassName("tab-link");
            for (i = 0; i < x.length; i++) {
                $(tablinks[i]).removeClass("active");
            }
            document.getElementById(tabName).style.display = "block";
            $(evt.currentTarget).addClass("active");
        }
    </script>
